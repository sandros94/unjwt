{
  "name": "unjwt",
  "version": "0.0.1",
  "description": "Low-level JWT utilities using unjs dependencies",
  "repository": "sandros94/unjwt",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@vitest/coverage-v8": "^3.0.9",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.23.0",
    "eslint-config-unjs": "^0.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@10.7.0"
}
