{
  "name": "unjwt",
  "version": "0.5.23",
  "description": "Low-level JWT utilities",
  "repository": "sandros94/unjwt",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./jwe": {
      "types": "./dist/jwe.d.mts",
      "default": "./dist/jwe.mjs"
    },
    "./jwk": {
      "types": "./dist/jwk.d.mts",
      "default": "./dist/jwk.mjs"
    },
    "./jws": {
      "types": "./dist/jws.d.mts",
      "default": "./dist/jws.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.mts",
      "default": "./dist/utils.mjs"
    },
    "./h3": {
      "types": "./dist/h3.d.mts",
      "default": "./dist/h3.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "bun --watch ./playground/main.ts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm prepack && changelogen --release --push --publish",
    "test": "pnpm lint && pnpm test:types && vitest run --slowTestThreshold=1000",
    "test:coverage": "pnpm lint && pnpm test:types && vitest run --coverage --slowTestThreshold=1000",
    "test:types": "tsc --noEmit --skipLibCheck",
    "bench": "vitest bench --run"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "automd": "^0.4.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.36.0",
    "eslint-config-unjs": "^0.5.0",
    "jose": "^6.1.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "cookie-es": "^1.2.2 || ^2.0.0",
    "h3": "^1.15.4 || ^2.0.0-rc.0 || ^2.0.0"
  },
  "peerDependenciesMeta": {
    "cookie-es": {
      "optional": true
    },
    "h3": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.17.1"
}
